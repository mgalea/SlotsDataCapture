@model SlotsDataCapture.ENTITIES.Contacts
@{ViewBag.Title = "Details";}

<div class="container-fluid">
    <div class="form-top-container">
        <div class="card border-0">
            <div class="card-header border-0">
                <div class="card bg-dark text-white">
                    <div class="card-body">
                        <h5 class="tl-sda-tl-bar">S.D.C<span class="tl-fm-detail">Detail</span>Record<span class="tl-fm-dynamic">@Html.DisplayFor(model => model.FName)  @Html.DisplayFor(model => model.LName)</span></h5>
                    </div>
                </div> @*Card Header Title Content*@
            </div> @*Card Header End*@

            <div class="card-body">
                <p class="form-location">Contact<span class="form-location-bold">Detail</span></p>
                <div class="row  form-row-form-content">
                    <div class="col-md-12 offset-md-1">
                        <!-- set form width -->
                        <div class="form-row mt-10">
                            <div class="container">
                                <div class="row">
                                    <div class="col-sm">
                                        <span class="form-style-details">
                                            <i class="fa fa-calendar" aria-hidden="true"></i>
                                            <i class="fa fa-1x  fa-institution" aria-hidden="true"></i>
                                        </span>
                                    </div>
                                    <div class="col-sm"></div>
                                    <div class="col-sm">
                                        <span class="form-style-details">
                                            <i class="fa fa-user" aria-hidden="true"></i>
                                            <i class="fa fa-1x  fa-institution" aria-hidden="true"></i>
                                        </span>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm"><span for="lbl_createdon" class="lbl-01">Created On</span></div>
                                    <div class="col-sm"></div>
                                    <div class="col-sm"><span for="lbl_opName" class="lbl-01">Contact For</span></div>
                                </div>
                                <div class="row">
                                    <div class="col-sm"><span class="lbl-001">@Html.DisplayFor(model => model.CreatedOn)</span></div>
                                    <div class="col-sm"></div>
                                    <div class="col-sm"><span class="lbl-001">@Html.DisplayFor(model => model.Tbl_OperatorVenues.Name)</span></div>
                                </div>
                                <div class="row form-spliter"></div>
                                <div class="row">
                                    <div class="col-sm">
                                        <span class="form-style-details">
                                            <i class="fa fa-1x  fa-institution"></i>
                                            <i class="fa fa-1x  fa-address-card"></i>
                                        </span>
                                    </div>
                                    <div class="col-sm"></div>
                                    <div class="col-sm">
                                        <span class="form-style-details">
                                            <i class="fa fa-1x  fa-institution"></i>
                                            <i class="fa fa-1x  fa-address-card"></i>
                                        </span>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm"><span for="lbl_compName" class="lbl-01">Company Name</span></div>
                                    <div class="col-sm"></div>
                                    <div class="col-sm"><span for="lbl_compPosition" class="lbl-01">Position Held</span></div>
                                </div>
                                <div class="row">
                                    <div class="col-sm"><span class="lbl-001">@Html.DisplayFor(model => model.Company)</span></div>
                                    <div class="col-sm"></div>
                                    <div class="col-sm"><span class="lbl-001">@Html.DisplayFor(model => model.Position)</span></div>
                                </div>
                                <div class="row form-spliter"></div>
                                <div class="row">
                                    <div class="col-sm">
                                        <span class="form-style-details">
                                            <i class="fa fa-1x  fa-institution"></i>
                                            <i class="fa fa-1x  fa-user"></i>
                                        </span>
                                    </div>
                                    <div class="col-sm"></div>
                                    <div class="col-sm">
                                        <span class="form-style-details">
                                            <i class="fa fa-1x  fa-institution" aria-hidden="true"></i>
                                            <i class="fa fa-1x  fa-user"></i>
                                        </span>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm"><span for="lbl_fname" class="lbl-01">First Name</span></div>
                                    <div class="col-sm"></div>
                                    <div class="col-sm"><span for="lbl_lname" class="lbl-01">Last Name</span></div>
                                </div>
                                <div class="row">
                                    <div class="col-sm"><span class="lbl-001">@Html.DisplayFor(model => model.FName)</span></div>
                                    <div class="col-sm"></div>
                                    <div class="col-sm"><span class="lbl-001">@Html.DisplayFor(model => model.LName)</span></div>
                                </div>
                                <div class="row form-spliter"></div>
                                <div class="row">
                                    <div class="col-sm">
                                        <span class="form-style-details">
                                            <i class="fa fa-1x  fa-mobile-phone" aria-hidden="true"></i>
                                        </span>
                                    </div>
                                    <div class="col-sm">
                                        <span class="form-style-details">
                                            <i class="fa fa-1x  fa-phone" aria-hidden="true"></i>
                                        </span>
                                    </div>
                                    <div class="col-sm">
                                        <span class="form-style-details">
                                            <i class="fa fa-1x  fa-at" aria-hidden="true"></i>
                                        </span>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm"><span for="lbl_mobile" class="lbl-01">Mobile</span></div>
                                    <div class="col-sm"><span for="lbl_phone" class="lbl-01">Phone</span></div>
                                    <div class="col-sm"><span for="lbl_email" class="lbl-01">Email</span></div>
                                </div>
                                <div class="row">
                                    <div class="col-sm"><span class="lbl-001">@Html.DisplayFor(model => model.Mobile)</span></div>
                                    <div class="col-sm"><span class="lbl-001">@Html.DisplayFor(model => model.Phone)</span></div>
                                    <div class="col-sm"><span class="lbl-001">@Html.DisplayFor(model => model.Email)</span></div>
                                </div>
                                <div class="row form-spliter"></div>
                                <div class="row">
                                    <div class="col-sm">
                                        <span class="form-style-details">
                                            <i class="fa fa-1x  fa-institution" aria-hidden="true"></i>
                                            <i class="fa fa-1x  fa-accessible-icon" aria-hidden="true"></i>
                                        </span>
                                    </div>
                                    <div class="col-sm"></div>
                                    <div class="col-sm">
                                        <span class="form-style-details">
                                            <i class="fa fa-1x  fa-institution" aria-hidden="true"></i>
                                            <i class="fa fa-1x  fa-globe" aria-hidden="true"></i>
                                        </span>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm"><span for="lbl_activity" class="lbl-01">Status?</span></div>
                                    <div class="col-sm"></div>
                                    <div class="col-sm"><span for="lbl_country" class="lbl-01">Country</span></div>
                                </div>
                                <div class="row">
                                    <div class="col-sm"><span class="lbl-001">@Html.DisplayFor(model => model.IsActiveDisplay)</span></div>
                                    <div class="col-sm"></div>
                                    <div class="col-sm"><span class="lbl-001">@Html.DisplayFor(model => model.Tbl_Countries.Name)</span></div>
                                </div>
                                <div class="row form-spliter"></div>
                            </div> @*Close Container*@
                        </div> @*Close form-row*@
                    </div>
                </div> @*Close content row*@
                <div class="row  form-row-form-controls">
                    <div class="col-sm-6">
                        <span class="form-style-details">
                            @Html.ActionLink("Edit Contacts", "Edit", new { id = Model.ContactID }, new { @class = "btn btn-outline-primary" })
                        </span>
                    </div>
                    <div class="col-sm-6">
                        <span class="form-style-details">
                            @Html.ActionLink("Back To Listings", "Index", "Companies", null, new { @class = "btn btn-outline-primary" })
                        </span>
                    </div>
                </div> @*Close form-row-form-controls*@
            </div> @*Close  row form controls*@
            <div class="card-header border-0">
                <div class="card bg-dark text-white">
                    <div class="card-body">
                        <div>
                            @Html.Partial("footer")
                        </div>
                    </div>
                </div> @*Card Header Title Content*@
            </div>
        </div>@*Close card-body*@
    </div> @*Close form-top-container*@
</div> @*Close Final*@







